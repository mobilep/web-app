{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2017,
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true
		},
		"sourceType": "module"
	},
	"extends": "eslint:recommended",
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true,
		"jest": true,
		"mocha": true,
		"node": true,
		"serviceworker": true,
		"worker": true
	},
	"rules": {
		"no-debugger": "warn",
		"no-console": "warn",
		"strict": "error"
	},
	"globals": {
		"System": true,
		"googletag": true
	}
}
