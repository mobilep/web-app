{
  "name": "mobilep-web",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "add-component": "node scripts/component-generator",
    "create-user": "node scripts/user-generator",
    "build-storybook": "build-storybook -s public",
    "build": "react-app-rewired build",
    "phase:build:production": "yarn install --production=true && REACT_APP_API_ENV=production yarn build",
    "eject": "react-scripts eject",
    "lint-css": "stylelint 'src/**/*.css'",
    "lint-js": "eslint ./src",
    "lint": "yarn lint-js && yarn lint-css",
    "prepush": "yarn lint && CI=true yarn test",
    "start": "react-app-rewired start",
    "storybook": "start-storybook -p 9009 -s public",
    "test-automation": "yarn test --testMatch=\"<rootDir>/automation/**/?(*.)(spec|test).{js,jsx,mjs}\"",
    "test": "TZ=UTC react-scripts test --env=jsdom",
    "start-electron": "cd ./electron && npm i && npm start",
    "build-electron-windows": "cd ./electron && npm i && npm run win"
  },
  "engines": {
    "node": ">=10 <11",
    "yarn": ">=1.9.0"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "array-flat-polyfill": "^1.0.1",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.0.8",
    "date-fns": "^2.1.0",
    "dialog-polyfill": "^0.4.10",
    "firebase": "^5.5.0",
    "formdata-polyfill": "^3.0.19",
    "hoist-non-react-statics": "^3.0.1",
    "intersection-observer": "^0.5.1",
    "plyr": "^3.5.10",
    "postcss-custom-properties": "^8.0.3",
    "prop-types": "^15.6.2",
    "react": "^16.9.0",
    "react-app-rewire-postcss": "^2.0.0",
    "react-app-rewired": "^1.6.2",
    "react-datepicker": "^2.9.6",
    "react-dom": "^16.9.0",
    "react-outline-manager": "^1.2.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "react-string-replace": "^0.4.4",
    "react-swipeable": "^4.3.0",
    "redux": "^4.0.5",
    "redux-observable": "^1.0.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.2.1",
    "shaka-player": "^2.3.7",
    "ua-parser-js": "^0.7.18",
    "webrtc-adapter": "^6.4.0"
  },
  "devDependencies": {
    "@dump247/storybook-state": "^1.5.0",
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-backgrounds": "^3.4.10",
    "@storybook/addon-knobs": "^3.4.10",
    "@storybook/addon-links": "^3.4.10",
    "@storybook/addon-options": "^3.4.10",
    "@storybook/addon-viewport": "^3.4.10",
    "@storybook/addons": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "babel-eslint": "^9.0.0",
    "babel-preset-minify": "^0.5.0-alpha.a24dd066",
    "enzyme": "^3.5.1",
    "enzyme-adapter-react-16": "^1.4.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.18.3",
    "faker": "^4.1.0",
    "husky": "^0.14.3",
    "inquirer": "^5.2.0",
    "jest-image-snapshot": "^2.5.0",
    "puppeteer": "1.5.0-next.1530061501767",
    "react-test-renderer": "^16.5.2",
    "storybook-react-router": "^1.0.8",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "resolutions": {
    "babel-preset-minify": "^0.5.0-alpha.a24dd066",
    "jest": "23.5.0"
  }
}
